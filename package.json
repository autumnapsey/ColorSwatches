{
  "name": "Colors",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Autumn Apsey <autumnapsey@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env production",
    "server": "nodemon server.js",
    "dev": "webpack --mode development && node server.js",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --mode development\"",
    "precommit": "lint-staged",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.1",
    "react-svg": "^10.0.20",
    "recompose": "^0.26.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "nodemon": "^1.17.2",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.11.1",
    "style-loader": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ],
    "rules": {
      "react/no-string-refs": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "flow",
          "trailingComma": "all"
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-wrap-multilines": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    },
    "plugins": [
      "prettier",
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "fetch": true
    }
  }
}
